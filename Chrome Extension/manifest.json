{
    "manifest_version": 3,
    "name": "Password Manager",
    "description": "My own password manager!",
    "version": "1.0",
    "action": {
      "default_popup": "main.html",
      "default_icon": "passkeyimg.png"
    },
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
    ]
  }
  